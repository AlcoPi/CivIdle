{
   "name": "CivIdle",
   "productName": "cividle",
   "version": "1.0.0",
   "description": "CivIdle is an idle/incremental game that allows you to lead your own civilization through thousands of years from ancient times to modern era",
   "main": "compiled/index.js",
   "scripts": {
      "build": "tsc",
      "start": "npm run build && electron-forge start",
      "generate": "tsc src/Generator.ts --esModuleInterop true --outDir compiled/ && node compiled/Generator.js",
      "package": "npm run build && electron-forge package",
      "make": "npm run build && electron-forge make",
      "publish": "npm run build && electron-forge publish",
      "lint": "echo \"No linting configured\""
   },
   "keywords": [],
   "author": {
      "name": "Fish Pond Studio",
      "email": "hi@fishpondstudio.com"
   },
   "license": "MIT",
   "dependencies": {
      "koffi": "2.3.4",
      "typescript": "^4.9.4"
   },
   "devDependencies": {
      "@electron-forge/cli": "^6.0.4",
      "@electron-forge/maker-deb": "^6.0.4",
      "@electron-forge/maker-rpm": "^6.0.4",
      "@electron-forge/maker-squirrel": "^6.0.4",
      "@electron-forge/maker-zip": "^6.0.4",
      "electron": "22.1.0",
      "fs-extra": "^11.1.0"
   }
}
